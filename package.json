{
  "name": "nar_api",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "nar_api": "bin/nar_api.js"
  },
  "overrides": {
    "@smithy/types": "3.3.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --require-approval never",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "_prepare": "npm run compile",
    "pretest": "npm run compile && npm run synth > /dev/null",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@smithy/util-stream": "^3.0.1",
    "@types/aws-lambda": "^8.10.137",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "20.12.5",
    "@types/supertest": "^6.0.2",
    "aws-cdk": "2.1006.0",
    "aws-sdk-client-mock": "^4.0.1",
    "aws-sdk-client-mock-jest": "^4.0.1",
    "esbuild": "^0.25.0",
    "gts": "^5.3.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.7.0",
    "@aws-lambda-powertools/tracer": "^2.7.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/client-sfn": "^3.645.0",
    "@aws-sdk/lib-storage": "^3.645.0",
    "@codegenie/serverless-express": "^4.14.0",
    "@middy/core": "^4.0.0",
    "aws-cdk-lib": "2.186.0",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "constructs": "^10.4.2",
    "express": "^5.0.1",
    "luxon": "^3.4.4",
    "source-map-support": "^0.5.21"
  }
}
